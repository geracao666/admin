{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run prisma:migrate && npm run prisma:seed && next dev",
    "build": "dotenv -e .env.management -- next build",
    "start": "next start",
    "lint": "next lint",
    "serve": "npx serve@latest out",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "management:prisma:migrate": "dotenv -e .env.management -- npm run prisma:migrate",
    "management:prisma:studio": "dotenv -e .env.management -- npm run prisma:studio",
    "management:prisma:seed": "dotenv -e .env.management -- npm run prisma:seed",
    "admin": "npm install && npm run management:prisma:migrate && npm run management:prisma:seed && dotenv -e .env.management -- next dev"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.3",
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.3",
    "axios-hooks": "^5.0.2",
    "bytes": "^3.1.2",
    "classnames": "^2.5.0",
    "hasha": "^6.0.0",
    "http-status-codes": "^2.3.0",
    "limiter": "^2.1.0",
    "mime-types": "^2.1.35",
    "next": "^14.1.3",
    "octokit": "^3.1.2",
    "react": "^18",
    "react-daisyui": "^5.0.0",
    "react-dom": "^18",
    "react-hook-form": "^7.49.2",
    "react-image-crop": "^11.0.4",
    "slugify": "^1.6.6",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.4",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.11.17",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
